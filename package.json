{
  "name": "weather-power-up",
  "version": "1.0.0",
  "description": "A small weather Power-Up for Trello",
  "main": "index.js",
  "repository": "git@github.com:trello/weather-power-up.git",
  "author": "Matt Cowan <mcowan@atlassian.com>",
  "license": "MIT",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "build": "parcel build src/html/*.html",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-interpolate-env": "^0.0.1",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "airbnb/base",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "staticFiles": {
    "staticPath": [
      "src/images",
      "src/strings"
    ]
  }
}
